<%= form_for @student do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :number %>
	<%= f.text_field :number %>
	<%= f.label :school %>
	<%= f.text_field :school %>
	<%= f.label :fee %>
	<%= f.text_field :fee %>
	<%= f.label :subject %>
	<%= f.collection_select(:subject_id, @subjects, :id, :name) %>
	<%= f.submit %>
<% end %>