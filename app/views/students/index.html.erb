<h1>All Students</h1>
<div><%= link_to "Create New Student", new_student_path %></div>

<% @students.each do |student| %>
	<%= link_to "#{student.number}", edit_student_path(student) %>
	<%= student.name %>
	<%= student.school %>
	<%= student.fee %>	
	<% student.subjects.each do |subject| %>
		<li><%= subject.name %></li>
	<% end %>

	<%= link_to "Delete", 
				student,
				:method => :delete %>
</br>
<% end %>
